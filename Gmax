import java.util.*;
class Gmax
{
    public static void main(String args[])
    {
    Scanner io=new Scanner(System.in);
    int n=io.nextInt();
    int a[]=new int[n];
    for(int i=0;i<a.length;i++)
    {
        a[i]=io.nextInt();
    }
    int max=a[0];
    for(int i=0;i<a.length;i++)
    {
        if(a[i]>max)
        {
        max=a[i];
        }
    }
    System.out.println(max);
    }
}
